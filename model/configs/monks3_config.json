{
	"train_set": "monks-3.test.csv",
    "output_path": "output/monks3/",
    "graph_path": "graphs/monks3/",
    "input_size": 17,
    "output_size": 1,
	
    "model": {
	"global_conf": {
		"task": "classification",
	    "epsilon": 1e-4,
	    "max_step": 300,
	    "check_step": 50,
		"metrics": {"train": ["mse", "accuracy"],
					"test":  ["mse", "accuracy"]
					},
		"preference metric": ["train", "mse"],
		"maxfold": 0,
		"patience": 4,
		"wc_threshold": 0
	},
	"hyperparameters":{
		"eta_decay": [-1, 25],
	    "eta": [1],
	    "lambda": [0],
	    "alpha": [0.8],
	    "batch_size": [-1],
	    "hidden_units": [
			[[2, "sigmoidal", 1], [2, "sigmoidal", 1], [1, "sigmoidal", 1]],
			[[2, "tanh", 1], [2, "tanh", 1], [1, "tanh", 1]]
		]
	}
    }
}
