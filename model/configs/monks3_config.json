{
	"data_conf":{
		"train_set": "monks-3.train.csv",
		"test_set": "monks-3.test.csv",
    	"output_path": "output/monks3/",
    	"graph_path": "graphs/monks3/",
		"input_size": 17,
		"output_size": 1
	},

	"global_conf": {
		"task": "classification",
	    "max_step": 300,
	    "check_step": 10,
		"validation": "trials",
		"maxfold": 3,
		"metrics": {"train": ["mse", "accuracy"],
			"test":  ["mse", "accuracy"]
		},
		"gs_preferred_metric": ["train", "accuracy"],
		"searched_hyper": ["eta", "lam", "alpha"],
		"loss_tolerance": 4e-2,
		"loss_patience": 1,
		"wc_tolerance": 0,
		"wc_patience": 1
	},

	"hyperparameters":{
		"eta_decay": [-1],
	    "eta": [6],
	    "lam": [1e-1, 0],
	    "alpha": [0.5],
	    "batch_size": [-1],
	    "hidden_units": [
			[["sigmoidal", 4], ["sigmoidal", 1]]
		]
	}
}