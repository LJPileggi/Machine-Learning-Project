{
	"data_conf":{
		"train_set": "mlcup_internaltrain.csv",
		"test_set": "mlcup_internaltest.csv",
		"output_path": "output/mlcup/",
		"graph_path": "graphs/mlcup/",
		"preprocess": "both_norm",
		"input_size": 10,
		"output_size": 2
	},

	
	"global_conf": {
		"task": "regression",
	    "epsilon": 1e-3,
	    "max_step": 200,
		"check_step": 20,
		"metrics": {"train": ["mee", "mse"],
					"val": 	 ["mee", "mse"]
					},
		"gs_preferred_metric": ["val", "mse"],
		"searched_hyper": ["eta", "lam", "alpha"],
		"maxfold": 1,
		"patience": 4,
		"wc_threshold": 0
	},

	"hyperparameters":{
	    "eta": [0.7],
	    "eta_decay": [-1],
	    "lam": [1e-6],
	    "alpha": [0.7],
	    "batch_size": [64],
	    "hidden_units": [
			[["tanh", 5], ["tanh", 5], ["linear", 2]]
		]
	}
}
