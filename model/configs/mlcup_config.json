{
    "train_set": "ML-CUP21-TR.csv",
    "test_set": "ML-CUP21-TR.csv",
    "output_path": "output/mlcup/",
    "graph_path": "graphs/mlcup/",
    "input_size": 10,
    "output_size": 2,
    "model": {
	"global_conf": {
	    "task": "regression",
	    "epsilon": 1e-1,
	    "max_step": 500,
	    "check_step": 50,
		"validation_metric": "mee",
		"patience": 4,
		"wc_threshold": 1.5,
	    "max_fold": 5
	},
	"hyperparameters":{
	    "eta": [1, 0.7, 0.5],
	    "eta_decay": [12, 25, 50],
	    "lambda": [0],
	    "alpha": [0.5],
	    "batch_size": [64],
	    "hidden_units": [
			[[32, "sigmoidal", 1], [32, "sigmoidal", 1], [2, "linear", 1]]
		]
	}
    }
}
