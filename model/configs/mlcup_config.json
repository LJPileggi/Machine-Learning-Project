{
	"data_conf":{
		"train_set": "mlcup_internaltrain.csv",
		"test_set": "mlcup_internaltest.csv",
		"output_path": "output/mlcup/",
		"graph_path": "graphs/mlcup/",
		"input_size": 10,
		"output_size": 2
	},

	"global_conf": {
	    "task": "regression",
	    "epsilon": 1e-1,
	    "max_step": 500,
	    "check_step": 50,
		"validation_metric": "mee",
		"patience": 4,
		"wc_threshold": 0,
	    "max_fold": 4
	},

	"hyperparameters":{
	    "eta": [1, 0.7, 0.5],
	    "eta_decay": [25],
	    "lambda": [1e-6, 1e-5, 1e-4],
	    "alpha": [0.3, 0.5, 0.7],
	    "batch_size": [64],
	    "hidden_units": [
			[[15, "sigmoidal", 1], [15, "sigmoidal", 1], [15, "sigmoidal", 1], [2, "linear", 1]],
			[[15, "tanh", 1], [15, "tanh", 1], [15, "tanh", 1], [2, "linear", 1]],
			[[20, "tanh", 1], [20, "tanh", 1], [20, "tanh", 1], [2, "linear", 1]]
		]
	}
    }
}
