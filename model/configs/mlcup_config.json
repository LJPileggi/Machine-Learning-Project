{
	"data_conf":{
		"train_set": "mlcup_internaltrain.csv",
		"test_set": "mlcup_internaltest.csv",
		"output_path": "output/mlcup/",
		"graph_path": "graphs/mlcup/",
		"input_size": 10,
		"output_size": 2
	},

	
	"global_conf": {
		"task": "regression",
	    "epsilon": 1e-1,
	    "max_step": 50,
		"check_step": 50,
		"metrics": {"train": ["mee", "mse"],
					"val": ["mee", "mse"],
					"test": ["mse", "mee"]
					},
		"maxfold": 1,
		"patience": 4,
		"wc_threshold": 0
	},

	"hyperparameters":{
	    "eta": [0.7],
	    "eta_decay": [25],
	    "lambda": [1e-4],
	    "alpha": [0.7],
	    "batch_size": [64],
	    "hidden_units": [
			[[2, "tanh", 1], [2, "tanh", 1], [2, "linear", 1]]
		]
	}
}
