{
    "train_set": "ML-CUP21-TR.csv",
    "test_set": "ML-CUP21-TR.csv",
    "output_path": "output/mlcup/",
    "graph_path": "graphs/mlcup/",
    "input_size": 10,
    "output_size": 2,

    "model": {
	"global_conf": {
	    "task": "regression",
	    "epsilon": 1e-1,
	    "max_step": 1000,
	    "check_step": 50,
		"validation_metric": "mee",
		"patience": [2],
	    "max_fold": 1
	},
	"hyperparameters":{
	    "eta": [1],
	    "eta_decay": [-1],
	    "lambda": [1],
	    "alpha": [0],
	    "batch_size": [1],
	    "hidden_units": [
			[[5, "tanh"], [5, "tanh"], [5, "tanh"], [2, "linear"]]
		]
	    
	}
    }
}
