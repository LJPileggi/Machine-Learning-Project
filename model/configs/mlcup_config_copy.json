{
    "train_set": "ML-CUP21-TR.csv",
    "test_set": "ML-CUP21-TR.csv",
    "output_path": "output/mlcup/",
    "graph_path": "graphs/mlcup/",
    "input_size": 10,
    "output_size": 2,

    "model": {
	"global_conf": {
	    "task": "regression",
	    "epsilon": 1e-1,
	    "max_step": 1500,
	    "check_step": 50,
		"validation_metric": "mse",
		"patience": 4,
		"wc_threshold": 0,
	    "max_fold": 1
	},
	"hyperparameters":{
	    "eta": [1],
	    "eta_decay": [25],
	    "lambda": [0],
	    "alpha": [0.5],
	    "batch_size": [1],
	    "hidden_units": [
			[[10, "sigmoidal"], [10, "sigmoidal"], [10, "sigmoidal"], [2, "linear"]]
		]
	}
    }
}
