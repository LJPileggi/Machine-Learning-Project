{
    "train_set": "ML-CUP21-TR.csv",
    "test_set": "ML-CUP21-TR.csv",
    "output_path": "output/mlcup/",
    "graph_path": "graphs/mlcup/",
    "input_size": 10,
    "output_size": 2,

    "model": {
	"global_conf": {
	    "task": "regression",
	    "epsilon": 1e-1,
	    "max_step": 1000,
	    "check_step": 50,
	    "max_fold": 1
	},
	"hyperparameters":{
	    "eta": [30],
	    "eta_decay": [25],
	    "lambda": [1e-4],
	    "alpha": [0],
	    "batch_size": [32],
	    "hidden_units": [
			[[3, "relu"], [5, "relu"], [5, "relu"], [5, "relu"], [2, "linear"]]
		],
	    "patience": [2]
	}
    }
}
