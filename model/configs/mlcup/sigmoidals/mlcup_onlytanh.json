{
    "train_set": "ML-CUP21-TR.csv",
    "test_set": "ML-CUP21-TR.csv",
    "output_path": "output/mlcup/",
    "graph_path": "graphs/mlcup/",
    "input_size": 10,
    "output_size": 2,

    "model": {
	"global_conf": {
	    "task": "regression",
	    "epsilon": 1e-2,
	    "max_step": 500,
	    "check_step": 50,
	    "max_fold": 1
	},
	"hyperparameters":{
	    "eta_decay": [25],
	    "eta": [1, 0.5, 0.3],
	    "lambda": [0, 1e-6],
	    "alpha": [0, 0.5],
	    "batch_size": [-1, 1, 32],
	    "hidden_units": [
			[[3, "tanh"], [3, "tanh"], [3, "tanh"], [2, "linear"]],
			[[5, "tanh"], [5, "tanh"], [5, "tanh"], [2, "linear"]]
		],
	    "patience": [2]
	}
    }
}
