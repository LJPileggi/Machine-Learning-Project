{
    "train_set": "ML-CUP21-TR.csv",
    "test_set": "ML-CUP21-TR.csv",
    "output_path": "output/mlcup/",
    "graph_path": "graphs/mlcup/",
    "input_size": 10,
    "output_size": 2,
	"preprocessing": "output_stand",

    "model": {
	"global_conf": {
	    "task": "regression",
		"validation": "crossval",
	    "epsilon": 1e-1,
	    "max_step": 400,
	    "check_step": 50,
		"validation_metric": "mee",
		"patience": [2],
	    "max_fold": 1
	},
	"hyperparameters":{
	    "eta": [1],
	    "eta_decay": [25],
	    "lambda": [0],
	    "alpha": [0],
	    "batch_size": [32],
	    "hidden_units": [
			[[5, "tanh"], [5, "tanh"], [5, "tanh"], [2, "linear"]]
		]
	    
	}
    }
}
