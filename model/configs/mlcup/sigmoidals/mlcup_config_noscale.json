{
	"data_conf":{
		"train_set": "mlcup_internaltrain.csv",
		"test_set": "mlcup_internaltest.csv",
		"output_path": "output/mlcup/",
		"graph_path": "graphs/mlcup/noscale",
		"input_size": 10,
		"output_size": 2
	},

	
	"global_conf": {
		"task": "regression",
		"validation": "crossval",
		"loss_tolerance": 1e-2,
		"loss_patience": 1,
		"wc_tolerance": 0,
		"wc_patience": 1,
		"max_step": 300,
		"check_step": 30,
		"metrics": {"train": ["mee", "mse"],
					"val": 	 ["mee", "mse"]
					},
		"gs_preferred_metric": ["val", "mee"],
		"searched_hyper": ["eta", "lam", "alpha"],
		"maxfold": 4
	},

	"hyperparameters":{
	    "eta": [0.12, 0.1, 0.08],
	    "eta_decay": [-1],
	    "lam": [1e-5, 1e-3],
	    "alpha": [0.3, 0.8],
	    "batch_size": [64],
	    "hidden_units": [
			[["sigmoidal", 32], ["sigmoidal", 32], ["sigmoidal", 32], ["linear", 2]], 
			[["sigmoidal", 20], ["sigmoidal", 20], ["sigmoidal", 20], ["linear", 2]], 

		]
	}
}
