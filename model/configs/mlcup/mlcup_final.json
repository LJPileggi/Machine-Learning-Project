{
    "train_set": "mlcup_internaltrain.csv",
    "test_set": "mlcup_internaltest.csv",
    "blind_set": "ML-CUP21-TS.csv",
    "output_path": "output/mlcup/",
    "graph_path": "graphs/mlcup/",
    "input_size": 10,
    "output_size": 2,
    "model": {
	"global_conf": {
	    "task": "regression",
		"validation": "crossval",
	    "epsilon": 1e-1,
	    "max_step": 100,
	    "check_step": 50,
		"validation_metric": "mee",
		"patience": 4,
		"wc_threshold": 0,
	    "max_fold": 4
	},
	"hyperparameters":{
	    "eta": 0.5,
	    "eta_decay": 25,
	    "lambda": 1e-6,
	    "alpha": 0.5,
	    "batch_size": 64,
	    "hidden_units": [
		[15, "tanh", 1],
		[15, "tanh", 1],
		[15, "tanh", 1],
		[2, "linear", 1]]
	}
    }
}
