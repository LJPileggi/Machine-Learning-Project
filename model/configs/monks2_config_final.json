{
	"data_conf":{
		"train_set": "monks-2.train.csv",
		"test_set": "monks-2.test.csv",
    	"output_path": "output/monks2/",
    	"graph_path": "graphs/monks2/",
		"input_size": 17,
		"output_size": 1
	},

	"global_conf": {
		"task": "classification",
	    "max_step": 300,
	    "check_step": 10,
		"maxfold": 10,
		"validation": "trials",
		"metrics": {"train": ["mse", "accuracy"],
			"test":  ["mse", "accuracy"]
		},
		"gs_preferred_metric": ["train", "accuracy"],
		"searched_hyper": ["eta", "lam", "alpha"],
		"loss_tolerance": 0,
		"loss_patience": 4,
		"wc_tolerance": 0,
		"wc_patience": 1
	},

	"hyperparameters":{
		"eta_decay": [-1],
	    "eta": [15],
	    "lam": [0],
	    "alpha": [0.7],
	    "batch_size": [-1],
	    "hidden_units": [
			[["sigmoidal", 2], ["sigmoidal", 1]]
		]
	}
}