{
	"data_conf":{
		"train_set": "monks-2.test.csv",
		"test_set": "monks-2.test.csv",
    	"output_path": "output/monks2/",
    	"graph_path": "graphs/monks2/",
		"input_size": 17,
		"output_size": 1
	},

	"global_conf": {
		"task": "classification",
	    "epsilon": 0,
	    "max_step": 500,
	    "check_step": 10,
		"metrics": {"train": ["mse", "accuracy"],
					"test":  ["mse", "accuracy"]
					},
		"gs_preferred_metric": ["train", "mse"],
		"searched_hyper": ["eta", "lam", "alpha"],
		"maxfold": 0,
		"patience": 4,
		"wc_threshold": 0
	},

	"hyperparameters":{
		"eta_decay": [-1],
	    "eta": [0.1],
	    "lam": [0],
	    "alpha": [0],
	    "batch_size": [1],
	    "hidden_units": [
			[["sigmoidal", 2], ["sigmoidal", 1]]
		]
	}
}